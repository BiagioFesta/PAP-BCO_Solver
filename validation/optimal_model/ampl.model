set V;

param G{V,V};

var vA{V} binary;
var vB{V} binary;
var vAB{V} binary;

minimize objective: sum {i in V} vAB[i];

subject to cons1{i in V}: vA[i]+vB[i]+vAB[i]=1;
subject to cons2{i in V, j in V}: G[i,j]=1 ==> vA[i]+vA[j]<=1;
subject to cons3{i in V, j in V}: G[i,j]=1 ==> vB[i]+vB[j]<=1;